            <class name="org.bank.test.DuplicateEmailAccountsTest"/>
        </classes>
    </test>
    <test name="Test 4">
        <classes>
            <class name="org.bank.test.UpdateAccountNumberTest"/>
        </classes>
    </test>
</suite>

