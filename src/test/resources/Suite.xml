<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Test Suite Bank Users">
    <listeners>
        <listener class-name="org.bank.utils.listener.Listener"/>
    </listeners>

    <test name="Test 1">
        <classes>
            <class name="org.bank.test.EmptyEndpointTest"/>
        </classes>
    </test>
    <test name="Test 2">
